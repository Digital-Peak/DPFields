{
	"name": "Digital-Peak/ccl",
	"type": "library",
	"description": "Cross CMS Library from Digital Peak",
	"keywords": ["cms", "framework", "library"],
	"homepage": "https://github.com/Digital-Peak/ccl",
	"license": "MIT",
	"config": {
		"platform": {
			"php": "5.6.0"
		}
	},
	"require-dev": {
		"phpunit/phpunit": "^5",
        "squizlabs/php_codesniffer": "^2",
		"satooshi/php-coveralls": "^1.0"
	},
	"autoload": {
		"psr-4": {
			"CCL\\": "src/CCL"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"CCL\\Tests\\": "tests/CCL"
		}
	}
}
